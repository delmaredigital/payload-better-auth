{
  "name": "@delmaredigital/payload-better-auth",
  "version": "0.4.3-dev.12",
  "description": "Better Auth adapter and plugins for Payload CMS",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./adapter": {
      "import": "./dist/adapter/index.js",
      "types": "./dist/adapter/index.d.ts",
      "default": "./dist/adapter/index.js"
    },
    "./plugin": {
      "import": "./dist/plugin/index.js",
      "types": "./dist/plugin/index.d.ts",
      "default": "./dist/plugin/index.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "types": "./dist/exports/client.d.ts",
      "default": "./dist/exports/client.js"
    },
    "./components": {
      "import": "./dist/exports/components.js",
      "types": "./dist/exports/components.d.ts",
      "default": "./dist/exports/components.js"
    },
    "./components/auth": {
      "import": "./dist/components/auth/index.js",
      "types": "./dist/components/auth/index.d.ts",
      "default": "./dist/components/auth/index.js"
    },
    "./components/twoFactor": {
      "import": "./dist/components/twoFactor/index.js",
      "types": "./dist/components/twoFactor/index.d.ts",
      "default": "./dist/components/twoFactor/index.js"
    },
    "./management": {
      "import": "./dist/exports/management.js",
      "types": "./dist/exports/management.d.ts",
      "default": "./dist/exports/management.js"
    },
    "./components/management": {
      "import": "./dist/components/management/index.js",
      "types": "./dist/components/management/index.d.ts",
      "default": "./dist/components/management/index.js"
    },
    "./rsc": {
      "import": "./dist/exports/rsc.js",
      "types": "./dist/exports/rsc.d.ts",
      "default": "./dist/exports/rsc.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "swc ./src -d ./dist --strip-leading-paths --watch",
    "dev-publish": "pnpm build && pnpm version prerelease --preid=dev --no-git-tag-version && pnpm publish --registry http://localhost:4873 --no-git-checks",
    "clean": "rm -rf dist",
    "generate:types": "tsx src/scripts/generate-types.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@better-auth/passkey": ">=1.4.18",
    "@payloadcms/next": ">=3.69.0",
    "@payloadcms/ui": ">=3.69.0",
    "better-auth": ">=1.4.0",
    "next": ">=15.4.8",
    "payload": ">=3.69.0",
    "react": ">=19.2.1"
  },
  "peerDependenciesMeta": {
    "@better-auth/passkey": {
      "optional": true
    }
  },
  "devDependencies": {
    "@better-auth/passkey": "^1.4.18",
    "@payloadcms/next": "^3.77.0",
    "@payloadcms/ui": "^3.77.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.15.11",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@vitest/coverage-v8": "^2.1.9",
    "better-auth": "^1.4.18",
    "next": "^16.1.6",
    "payload": "^3.77.0",
    "react": "^19.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "payload-plugin",
    "better-auth",
    "authentication",
    "adapter"
  ],
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "author": "Delmare Digital",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delmaredigital/payload-better-auth.git"
  },
  "bugs": {
    "url": "https://github.com/delmaredigital/payload-better-auth/issues"
  },
  "homepage": "https://delmaredigital.com/developers"
}
