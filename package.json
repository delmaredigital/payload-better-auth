{
  "name": "@delmaredigital/payload-better-auth",
  "version": "0.1.3",
  "description": "Better Auth adapter and plugins for Payload CMS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./adapter": {
      "types": "./dist/adapter/index.d.ts",
      "import": "./dist/adapter/index.mjs",
      "require": "./dist/adapter/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.mjs",
      "require": "./dist/plugin/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "better-auth": ">=1.0.0",
    "next": ">=15.4.0",
    "payload": ">=3.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "better-auth": "^1.4.10",
    "next": "^16.1.1",
    "payload": "^3.69.0",
    "react": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "payload-plugin",
    "better-auth",
    "authentication",
    "adapter"
  ],
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "author": "Delmare Digital",
  "license": "PolyForm-Noncommercial-1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delmaredigital/payload-better-auth.git"
  },
  "bugs": {
    "url": "https://github.com/delmaredigital/payload-better-auth/issues"
  },
  "homepage": "https://delmaredigital.com/developers"
}
